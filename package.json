{
  "name": "nestjs-stripe",
  "version": "1.0.0",
  "description": "NestJS Firebase Admin Module + Passport",
  "author": "Alexander Requelme",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "pnpm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "stripe"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexrequelme/nestjs-stripe"
  },
  "bugs": "https://github.com/alexrequelme/nestjs-stripe",
  "peerDependencies": {
    "@nestjs/common": ">=10.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.8.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.5.0",
    "prettier": "3.0.0",
    "stripe": "^13.8.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.0.0",
    "tsc-watch": "6.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
